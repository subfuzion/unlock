{
  "name": "gcloud-unlock-api",
  "version": "0.1.0-preview.12",
  "private": true,
  "description": "API to demonstrate generating terminal content",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "test": "prettier --check . && node test/test.js http://localhost:8080",
    "fix": "prettier --write .",
    "deploy": "gcloud run deploy gcloud-unlock-api --allow-unauthenticated --timeout=30 --min-instances=1 --source .",
    "watch": "nodemon || true"
  },
  "dependencies": {
    "@subfuzion/terminal-md": "^0.1.0",
    "express": "4.18.2",
    "ink": "4.4.1"
  },
  "devDependencies": {
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "nodemon": "3.0.1",
    "prettier": "3.0.3"
  }
}
