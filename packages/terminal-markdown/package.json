{
  "name": "terminal-markdown",
  "description": "Render markdown for terminals",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "md2ascii": "./bin/md2ascii"
  },
  "files": [
    "dist/src/**.*"
  ],
  "types": "./types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "dist/src/index.d.ts": [
        "dist/src/index.d.ts"
      ],
      "*": [
        "dist/src/*"
      ]
    }
  },
  "exports": {
    ".": "./dist/src/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "check": "prettier --check .",
    "prebuild": "npm run check",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint .",
    "fix": "prettier --write .",
    "test": "node --no-warnings=ExperimentalWarning --loader ts-node/esm test/test.ts",
    "test:watch": "node --watch --no-warnings=ExperimentalWarning --loader ts-node/esm test/test.ts"
  },
  "devDependencies": {
    "@types/mdast": "4.0.3",
    "@types/node": "20.9.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "ascii-art-image": "1.4.0",
    "remark": "15.0.1",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.0",
    "remark-parse": "11.0.0",
    "yaml": "2.3.4"
  }
}
